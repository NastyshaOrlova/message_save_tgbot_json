generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // это бд
  url      = env("DATABASE_URL") // это ее url
}

model Message {
  id          Int       @id() @default(autoincrement()) @unique
  content     String
  timestamp   DateTime @default(now())
  user_id     Int

  @@map("messages")
}