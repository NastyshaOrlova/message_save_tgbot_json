generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // это бд
  url      = env("DATABASE_URL") // это ее url
}

model Message {
  id          Int       @id() @default(autoincrement()) @unique
  message     String
  message_date DateTime
  user_id     Int
  user        User      @relation(fields: [user_id], references: [id])

  @@map("messages")
}