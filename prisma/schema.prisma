generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // это бд
  url      = env("DATABASE_URL") // это ее url
}

model User {
  id     Int @id() @default(autoincrement()) @unique
}

model Message {
  id           Int    @id() @default(autoincrement()) @unique
  content      String
  userId       Int
  messageIndex Int
}